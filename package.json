{
  "name": "lit-blogpost",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "10.15.1"
  },
  "scripts": {
    "build": "babel ./server/src --out-dir ./server/dist",
    "start": "npm run db:migrate:fresh && npm run dev",
    "dev": "nodemon --exec babel-node ./server/src/index.js",
    "test": "jest --runInBand --detectOpenHandles --forceExit",
    "coveralls": "jest --runInBand --coverage --detectOpenHandles --forceExit && cat ./coverage/lcov.info | coveralls",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:migrate:fresh": "node_modules/.bin/sequelize db:migrate:undo:all && node_modules/.bin/sequelize db:migrate",
    "db:migrate:seed": "node_modules/.bin/sequelize db:migrate:undo:all && node_modules/.bin/sequelize db:migrate && node_modules/.bin/sequelize db:seed:all",
    "db:seed": "node_modules/.bin/sequelize db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oesukam/lit-blogpost.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oesukam/lit-blogpost/issues"
  },
  "homepage": "https://github.com/oesukam/lit-blogpost#readme",
  "dependencies": {
    "@babel/node": "^7.2.2",
    "bcrypt": "^3.0.3",
    "celebrate": "^9.0.2",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "jsonwebtoken": "^8.4.0",
    "moment": "^2.24.0",
    "nodemailer": "^5.1.1",
    "pg": "^7.8.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0",
    "uuid": "^3.3.2",
    "winston": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.0.0",
    "babel-plugin-root-import": "^6.1.0",
    "babel-preset-airbnb": "^3.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-prettier": "^3.0.1",
    "jest-cli": "^24.0.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.1",
    "supertest": "^3.4.2"
  },
  "jest": {
    "transform": {
      "^.+\\.js?$": "babel-jest"
    }
  }
}
