{
  "env": {
    "node": true,
    "es6": true,
    "jest/globals": true
  },
  "settings": {
    "import/resolver": {
      "babel-plugin-root-import": {
        "rootPathPrefix": "~",
        "rootPathSuffix": "server/src"
      }
    }
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["jest", "prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "import/prefer-default-export": "off",
    "consistent-return": 0
  }
}
